ifeq ($(OS), Windows_NT)
else

all:
	g++ -D ENABLE_SOUND=$(ENABLE_SOUND) $(GCC_FLAGS) $(GCC_INCLUDES) $(PLATFORM_FLAGS) $(PLATFORM_FILE) $(SOURCE_FILES) -o RacingToHell$(EXTENSION) $(GCC_LINKER)

optimized:
	g++ -O3 -DENABLE_SOUND=$(ENABLE_SOUND) $(GCC_INCLUDES) $(PLATFORM_FLAGS) $(PLATFORM_FILE) $(SOURCE_FILES) -o RacingToHell$(EXTENSION) $(GCC_LINKER)

endif